<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:commons="sap.ui.commons">
	<Dialog title="{i18n>powerMeterTitle}" afterClose="afterPowerMeterDialogClose"
		stretch="true" verticalScrolling="true" horizontalScrolling="false" contentHeight="100%">
		<content>


					<Panel expandable="true" expanded="true" headerText="Grafana Data" expand="expandGrafana">
				<content>
					<core:HTML id="grafanaData" content="">
					</core:HTML> </content>
			</Panel>


		</content>
		<beginButton>
			<Button text="{i18n>refresh}" press="powerMeterLoad" />
		</beginButton>
		<endButton>
			<Button text="{i18n>close}" press="powerMeterDialogClose" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>
