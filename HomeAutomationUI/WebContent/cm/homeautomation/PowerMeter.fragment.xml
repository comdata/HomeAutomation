<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:commons="sap.ui.commons">
	<Dialog title="{powermeter>/name}" afterClose="afterPowerMeterDialogClose"
		stretch="true">
		<content>

			<Panel expandable="false" expanded="true" headerText="Historic Data"
				width="auto" class="sapUiResponsiveMargin" expand="">
				<content>

					<core:HTML
						content="&lt;div id='chart'&gt;
    &lt;svg style='height:300px; width:100%;'&gt;&lt;/svg&gt;
&lt;/div&gt;"></core:HTML>
				</content>
			</Panel>

		</content>
		<beginButton>
			<Button text="Schliessen" press="powerMeterDialogClose" />
		</beginButton>
	</Dialog>
</core:FragmentDefinition>