<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:commons="sap.ui.commons"
	xmlns:agraebe.custom="agraebe.custom.control" >
	<Dialog title="{powermeter>/name}" afterClose="afterPowerMeterDialogClose"
		stretch="true">
		<content>

			<Panel expandable="false" expanded="true" headerText="Historic Data"
				width="auto" class="sapUiResponsiveMargin" expand="">
				<content>

					<agraebe.custom:ChartJSControl width="400" height="200" responsive="true" chartType="Line" data="{c3jsData>/lineData}"/>
				</content>
			</Panel>

		</content>
		<beginButton>
			<Button text="Schliessen" press="powerMeterDialogClose" />
		</beginButton>
	</Dialog>
</core:FragmentDefinition>